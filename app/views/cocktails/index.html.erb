<h1>Cocktails#index</h1>
<p>Find me in app/views/cocktails/index.html.erb</p>
<hr>
<ul class="list-unstyled">
  <% @cocktails.each do |cocktail| %>
    <li><%= link_to cocktail.name.downcase.capitalize, cocktail_path(cocktail) %></li>
  <% end %>
</ul>
<p class="mt-3"><%= link_to raw('<i class="fas fa-plus"></i> add new cocktail'), new_cocktail_path %></p>
